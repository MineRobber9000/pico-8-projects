pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--c1=cube 1
--c2=cube 2
--m =merged poly

function _init()
	c1={{x=40;y=40;};
					{x=60;y=40;};
					{x=60;y=60;};
					{x=40;y=60;};
				};
	c2={{x=80;y=80;};
					{x=100;y=80;};
					{x=100;y=100;};
					{x=80;y=100;};
				};
	m={};
	combine(c1,m);
	combine(c2,m);
end

function _draw()
	cls();
	draw_poly(c1,7);
	draw_poly(c2,7);
	draw_poly(m,8);
	print(m[1].x,0,0,14);
	print(m[1].y,0,8,14);
end

function _update60()
	--if btnp(0,0) then
		graham_scan(m);	
	--end
end

function graham_scan(points)
	set_start(points);
	sort(points);
end

function ccw(p1,p2,p3)
	return (p2.x-p1.x)*(p3.y-p1.y)-(p2.y-p1.y)*(p3.x-p1.x);
end

function sort(points)
	local o = points[1];
	local x_axis={{x=0;y=0;};
															{x=1;y=0;};
														};
														
	for i=2,#points do
		for j=2,#points do
			local p1=points[i];
			local p2=points[j];
			local l1={{x=o.x;y=o.y;};
													{x=p1.x;y=p1.y;};
												};
			local l2={{x=o.x;y=o.y;};
													{x=p2.x;y=p2.y;};
												};
			local angle1=get_angle(l1,x_axis);
			local angle2=get_angle(l2,x_axis);
			if angle1 > angle2 then
				p2.x,p1.x = p1.x,p2.x;
				p2.y,p1.y = p1.y,p2.y;
			end
		end
	end
end

function get_angle(l1)
	local dx=l1[1].x - l1[2].x;
	local dy=l1[1].y - l1[2].y;
	return atan2(dy,dx);
end

function combine(target,destination)
	for t in all(target) do
		add(destination, t);
	end
end

function draw_poly(list,col)
	for i=1,#list do
		local j=i+1;
		if j > #list then
			j=1;
		end
		line(list[i].x,list[i].y,list[j].x,list[j].y,col);
	end
end

function set_start(points)
	for p1 in all(points) do
		for p2 in all(points) do
			if p2.y < p1.y then
				p2.x,p1.x = p1.x,p2.x;
				p2.y,p1.y = p1.y,p2.y;
			elseif p2.y == p1.y then
				if p2.x > p1.x then
					p2.x,p1.x = p1.x,p2.x;
					p2.y,p1.y = p1.y,p2.y;
				end
			end
		end
	end
end
