pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
mouse={x=stat(32);y=stat(33);};
function mouse:draw()
	spr(2,mouse.x-3,mouse.y-3);
end
function mouse:update()
	poke(0x5f2d, 1);
	mouse.x=stat(32);
	mouse.y=stat(33);
end

-- s = line start
-- e = line end
-- n = normalized normal
-- f = facing mouse
-- p = projection points
-- v = shadow volume
walls={};
function walls.add(p1,p2)
	local dx=p2.x-p1.x;
	local dy=p2.y-p1.y;
	local dl=sqrt(dx*dx+dy*dy);
	local dx/=dl;
	local dy/=dl;
	local normal={x=p1.x+-dy;y=p1.y+dx};
	add(walls,{s=p1;e=p2;n=normal;f=true;p={s=p1,e=p2};v={};});
end
function walls:init()
	for map_y=0,15 do
		for map_x=0,15 do
			if fget(mget(map_x,map_y),0) then
				local x1=map_x*8;
				local y1=map_y*8;
				local x2=x1;
				local y2=y1+7;
				local x3=x1+7;
				local y3=y1+7;
				local x4=x1+7;
				local y4=y1;

				local left = fget(mget(map_x-1,map_y),0);
				local under = fget(mget(map_x,map_y+1),0);
				local right = fget(mget(map_x+1,map_y),0);
				local above  = fget(mget(map_x,map_y-1),0);

				if not left then
					walls.add({x=x1;y=y1;},{x=x2;y=y2;});
				end
				if not under then
			 		walls.add({x=x2;y=y2;},{x=x3;y=y3;});
			 	end
				if not right then
					walls.add({x=x3;y=y3;},{x=x4;y=y4;});
				end
				if not above then
					walls.add({x=x4;y=y4;},{x=x1;y=y1;});
				end
			end
		end
	end
end

function walls:update()
	for w in all(walls) do
		local l_x=mouse.x;
		local l_y=mouse.y;
		local n_x=w.n.x;
		local n_y=w.n.y;
		local x1=w.s.x;
		local y1=w.s.y;
		local x2=w.e.x;
		local y2=w.e.y;
		local d_l=(l_x-x1)*(y2-y1)-(l_y-y1)*(x2-x1);
		local d_n=(n_x-x1)*(y2-y1)-(n_y-y1)*(x2-x1);
		w.f=(d_l < 0 and d_n < 0);
		if w.f then
			w.p.s=get_projection(mouse,w.s);
			w.p.e=get_projection(mouse,w.e);
			w.v=polyfill({w.s;w.e;w.p.e;w.p.s});
		end
	end
end

function polyfill(p)
	local ymax=0;
	local ymin=128;

	for i=1,#p do
		ymax=max(ymax,p[i].y);
		ymin=min(ymin,p[i].y);
	end

	local tmp={};
	for y=ymin,ymax do
		if (y > 0 and y < 128) then
			for i=1,#p do
				local x1=p[i].x;
				local y1=p[i].y;
				local x2=0;
				local y2=0;
				if i>=#p then
					x2=p[1].x;
					y2=p[1].y;
				else
					x2=p[i+1].x;
					y2=p[i+1].y;
				end
				local dx=x2-x1;
				local dy=y2-y1;
				local x=x1-dx/dy*(y1-y);
				if ((y1<=y and y2>y)
					or (y2<=y and y1>y)) then
						add(tmp,{x=flr(x+0.5);y=y});
				end
			end
		end
	end
	return tmp;
end

function get_projection(p1,p2)
	local d={x=p2.x-p1.x;y=p2.y-p1.y};
	local d_l = sqrt(d.x * d.x + d.y * d.y);
	local norm_d={x=d.x/d_l;y=d.y/d_l;};
	local x=p2.x+norm_d.x*128*2;
	local y=p2.y+norm_d.y*128*2;
	return {x=x;y=y};
end

function walls:draw()
	for w in all(walls) do
		if w.f then
			draw_shadow(w.v);
		end
	end
end

function draw_shadow(p)
	for i=1,#p-1,2 do
		local x1=p[i].x;
		local y1=p[i].y;
		local x2=p[i+1].x;
		local y2=p[i+1].y;
		line(x1,y1,x2,y1,0);
	end
end

function _init()
	walls.init();
end

function _draw()
	cls();
	rectfill(0,0,128,128,5);
	mouse.draw();
	walls.draw();
	mapdraw();
	for w in all(walls) do
	--	if w.f then
			line(w.s.x,w.s.y,w.e.x,w.e.y,11);
			pset(w.n.x-(w.s.x-w.e.x)*0.5,w.n.y-(w.s.y-w.e.y)*0.5,9);
	--	end
	end
end

function _update60()
	mouse.update();
	walls.update();
end
