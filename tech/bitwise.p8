pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
	bitmap={};
	map_to_bit(bitmap);
	walls={};
	add_walls(walls,bitmap);
end

function add_walls(walls,b)
	for y=0,15 do
		for x=0,15 do
			bit=b[x+y*16+1].b;
			local x1=x*8;
			local y1=y*8;
			local x2=x1;
			local y2=y1+8;
			local x3=x1+8;
			local y3=y1+8;
			local x4=x1+8;
			local y4=y1;
			
			if bit==4 
				or bit==5
				or bit==6
				or bit==7
				or bit==12
				or bit==13
				or bit==14 
				or bit==15 then
				add_line(walls,{x=x1;y=y1;},{x=x2;y=y2;});
			end
			if bit==2 
				or bit==3
				or bit==6
				or bit==7
				or bit==10
				or bit==11
				or bit==14 
				or bit==15 then
				add_line(walls,{x=x2;y=y2;},{x=x3;y=y3;});
			end
			if bit==8 
				or bit==9
				or bit==10
				or bit==11
				or bit==12
				or bit==13
				or bit==14 
				or bit==15 then
				add_line(walls,{x=x3;y=y3;},{x=x4;y=y4;});
			end
			if bit==1 
				or bit==3
				or bit==5
				or bit==7
				or bit==9
				or bit==11
				or bit==13 
				or bit==15 then
				add_line(walls,{x=x4;y=y4;},{x=x1;y=y1;});
			end
		end
	end
end

function add_line(walls,p1,p2)
	local dx=p2.x-p1.x;
	local dy=p2.y-p1.y;
	local dl=sqrt(dx*dx+dy*dy);
	local dx/=dl;
	local dy/=dl;
	local normal={x=p1.x+-dy;y=p1.y+dx}; 
	add(walls,{s=p1;e=p2;n=normal;f=true;p={s=p1,e=p2};v={};});
end

function _draw()
	cls();
	rectfill(0,0,128,128,5);
	mapdraw();
	for w in all(walls) do
		if w.f then
			line(w.s.x,w.s.y,w.e.x,w.e.y,11);
			pset(w.n.x-(w.s.x-w.e.x)*0.5,w.n.y-(w.s.y-w.e.y)*0.5,9);
			--draw_shadow(w.v);
		end
	end
--[[	for b in all(bitmap) do
		if bor(0x00,b.b) > 0x00 then
			print(b.b,b.x*8,b.y*8,7);
		end
	end]]
	mouse.draw();
end

function draw_shadow(p)
	for i=1,#p-1,2 do
		local x1=p[i].x;
		local y1=p[i].y;
		local x2=p[i+1].x;
		local y2=p[i+1].y;
		line(x1,y1,x2,y1,0);
	end
end

function _update60()
	mouse.update();
	update_walls();
end

function update_walls()
	for w in all(walls) do
		local l_x=mouse.x;
		local l_y=mouse.y;
		local n_x=w.n.x;
		local n_y=w.n.y;
		local x1=w.s.x;
		local y1=w.s.y;
		local x2=w.e.x;
		local y2=w.e.y;
		local d_l=(l_x-x1)*(y2-y1)-(l_y-y1)*(x2-x1);
		local d_n=(n_x-x1)*(y2-y1)-(n_y-y1)*(x2-x1);
		w.f=(d_l < 0 and d_n < 0);		
		if w.f then
			w.p.s=get_projection(mouse,w.s);
			w.p.e=get_projection(mouse,w.e);
			w.v=polyfill({w.s;w.e;w.p.e;w.p.s});
		end
	end
end

function polyfill(p)
	local ymax=0;
	local ymin=128;

	for i=1,#p do
		ymax=max(ymax,p[i].y);
		ymin=min(ymin,p[i].y);
	end

	local tmp={};
	for y=ymin,ymax do
		if (y > 0 and y < 128) then
			for i=1,#p do
				local x1=p[i].x;
				local y1=p[i].y;
				local x2=0;
				local y2=0;
				if i>=#p then
					x2=p[1].x;
					y2=p[1].y;
				else
					x2=p[i+1].x;
					y2=p[i+1].y;
				end
				local dx=x2-x1;
				local dy=y2-y1;
				local x=x1-dx/dy*(y1-y);
				if ((y1<=y and y2>y)
					or (y2<=y and y1>y)) then
						add(tmp,{x=flr(x+0.5);y=y});
				end
			end
		end
	end
	return tmp;
end

function get_projection(p1,p2)
	local d={x=p2.x-p1.x;y=p2.y-p1.y};
	local d_l = 
	
	sqrt(d.x * d.x + d.y * d.y);
	local norm_d={x=d.x/d_l;y=d.y/d_l;};
	local x=p2.x+norm_d.x*128*2;
	local y=p2.y+norm_d.y*128*2;
	return {x=x;y=y};
end

function map_to_bit(bitmap)
	for y=0,15 do
		for x=0,15 do
			if fget(mget(x,y),0) then
				local b=0x00;
				--up
				if not fget(mget(x,y-1),0) then
					b=bor(b,0x01);
				end
				--down
				if not fget(mget(x,y+1),0) then
					b=bor(b,0x02);
				end
				--left
				if not fget(mget(x-1,y),0) then
					b=bor(b,0x04);
				end
				--right
				if not fget(mget(x+1,y),0) then
					b=bor(b,0x08);
				end
				add(bitmap,{x=x;y=y;b=b;});
			else
				add(bitmap,{x=x;y=y;b=0x00;});
			end
		end
	end
end

mouse={x=stat(32);y=stat(33);};
function mouse:draw()
	spr(2,mouse.x-3,mouse.y-3);
end
function mouse:update()
	poke(0x5f2d, 1);
	mouse.x=stat(32);
	mouse.y=stat(33);
end
